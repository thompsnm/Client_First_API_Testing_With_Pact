{
  "consumer": {
    "name": "Restaurant Rating Consumer"
  },
  "provider": {
    "name": "Restaurant Service"
  },
  "interactions": [
    {
      "description": "a request for information about a restaurant",
      "request": {
        "method": "get",
        "path": "/restaurant",
        "query": "name=Gondolier"
      },
      "response": {
        "status": 200,
        "headers": {
          "Content-Type": "application/json"
        },
        "body": {
          "name": "Gondolier",
          "rating": 5
        },
        "matchingRules": {
          "$.body.name": {
            "match": "type"
          },
          "$.body.rating": {
            "match": "type"
          }
        }
      }
    },
    {
      "description": "a request for the name of the top rated restaurant",
      "request": {
        "method": "get",
        "path": "/topRated"
      },
      "response": {
        "status": 200,
        "headers": {
          "Content-Type": "application/json"
        },
        "body": {
          "name": "Gondolier",
          "rating": 5
        },
        "matchingRules": {
          "$.body.name": {
            "match": "type"
          },
          "$.body.rating": {
            "match": "type"
          }
        }
      }
    }
  ],
  "metadata": {
    "pactSpecificationVersion": "2.0.0"
  }
}